{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "npx tsdx build && node dist/index.js",
    "build": "npx tsdx build && node dist/index.js",
    "test": "npx tsdx test",
    "lint": "npx tsdx lint",
    "prepare": "npx tsdx build",
    "size": "npx size-limit",
    "analyze": "npx size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "chat",
  "author": "Dhaniel Brand√£o",
  "module": "dist/.esm.js",
  "size-limit": [
    {
      "path": "dist/.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/nodemailer": "^6.4.4",
    "husky": "^8.0.1",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.7.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "mongoose": "^6.3.6",
    "morgan": "^1.10.0",
    "nodemailer": "^6.7.5",
    "sequelize": "^6.20.1",
    "sqlite3": "^5.0.8"
  }
}
